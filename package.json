{
  "name": "shardis",
  "version": "0.0.0",
  "description": "shardis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shardis/shardis.git"
  },
  "author": "Shardis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shardis/shardis/issues"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@angular/material": "2.0.0-alpha.9-3",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "normalize.css": "5.0.0",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "1.1.2",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/hammerjs": "2.0.33",
    "@types/jasmine": "2.5.36",
    "@types/node": "6.0.46",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "~1.0.0-beta.3",
    "coveralls": "^2.11.14",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "npm-check-updates": "2.8.6",
    "protractor": "4.0.10",
    "rimraf": "2.5.4",
    "ts-node": "1.6.1",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "webdriver-manager": "10.2.6"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf coverage src/main/resources/static/*",
    "clean:dist": "rimraf typings src/main/resources/static/*",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "coveralls": "cat ./coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "e2e:live": "protractor --elementExplorer",
    "lint": "tslint \"src/main/typescript/**/*.ts\"",
    "pretest": "rimraf coverage && npm run lint",
    "test": "ng test -w false --code-coverage",
    "test:live": "ng test",
    "pretest:phantom": "rimraf coverage && npm run lint",
    "test:phantom": "ng test -w false --browsers PhantomJS --code-coverage",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "ng build -dev",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "ng build -prod",
    "server": "npm run server:dev",
    "server:dev": "ng serve --p 4200 -pc proxy.json -dev",
    "server:prod": "ng serve --p 4200 -pc proxy.json -prod",
    "ncu": "ncu -a",
    "ng:upgrade": "ng init -n shardis -sd src/main/typescript --style scss --prefix shardis --routing",
    "start": "npm run server:dev"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
  }
}
